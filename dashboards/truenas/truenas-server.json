      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "auto", "textMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "HTTPS Probe",
      "type": "stat",
      "targets": [
        { "expr": "probe_success{job=\"truenas-probe\"}", "legendFormat": "Probe Status", "refId": "A" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
