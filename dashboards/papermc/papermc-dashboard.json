{
  "title": "PaperMC Server Monitoring",
  "description": "Container metrics for PaperMC Minecraft server",
  "tags": [
    "minecraft",
    "papermc",
    "kubernetes",
    "container"
  ],
  "style": "",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "fiscalYearStartMonth": 0,
  "refresh": "30s",
  "schemaVersion": 36,
  "panels": [
    {
      "type": "stat",
      "targets": [
        {
          "expr": "up{pod=~\"$pod\"}",
          "legendFormat": "Status",
          "refId": ""
        }
      ],
      "title": "Status",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"$pod\", container=\"papermc\"}[5m])) / sum(container_spec_cpu_quota{pod=~\"$pod\", container=\"papermc\"} / container_spec_cpu_period{pod=~\"$pod\", container=\"papermc\"}) * 100",
          "legendFormat": "CPU %",
          "refId": ""
        }
      ],
      "title": "CPU Usage",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{pod=~\"$pod\", container=\"papermc\"}) / sum(container_spec_memory_limit_bytes{pod=~\"$pod\", container=\"papermc\"}) * 100",
          "legendFormat": "Memory %",
          "refId": ""
        }
      ],
      "title": "Memory Usage",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "time() - container_start_time_seconds{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Uptime",
          "refId": ""
        }
      ],
      "title": "Uptime",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"$pod\", container=\"papermc\"}[5m]))",
          "legendFormat": "CPU cores",
          "refId": ""
        },
        {
          "expr": "sum(container_spec_cpu_quota{pod=~\"$pod\", container=\"papermc\"} / container_spec_cpu_period{pod=~\"$pod\", container=\"papermc\"})",
          "legendFormat": "Limit",
          "refId": ""
        }
      ],
      "title": "CPU Usage",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(container_cpu_cfs_throttled_seconds_total{pod=~\"$pod\", container=\"papermc\"}[5m])",
          "legendFormat": "Throttled seconds/s",
          "refId": ""
        },
        {
          "expr": "rate(container_cpu_cfs_throttled_periods_total{pod=~\"$pod\", container=\"papermc\"}[5m]) / rate(container_cpu_cfs_periods_total{pod=~\"$pod\", container=\"papermc\"}[5m]) * 100",
          "legendFormat": "Throttled periods %",
          "refId": ""
