{
  "title": "PaperMC Server Monitoring",
  "description": "Minecraft server metrics and logs",
  "tags": [
    "minecraft",
    "papermc",
    "kubernetes"
  ],
  "style": "",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "fiscalYearStartMonth": 0,
  "refresh": "30s",
  "schemaVersion": 36,
  "panels": [
    {
      "type": "gauge",
      "targets": [
        {
          "expr": "mc_tps{pod=~\"$pod\"}",
          "legendFormat": "TPS",
          "refId": ""
        }
      ],
      "title": "TPS",
      "description": "Ticks per second (target: 20)",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 20
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "sum(mc_players_online_total{pod=~\"$pod\"})",
          "legendFormat": "Online",
          "refId": ""
        }
      ],
      "title": "Players Online",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "mc_players_total{pod=~\"$pod\"}",
          "legendFormat": "Total",
          "refId": ""
        }
      ],
      "title": "Total Players",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "sum(mc_loaded_chunks_total{pod=~\"$pod\"})",
          "legendFormat": "Chunks",
          "refId": ""
        }
      ],
      "title": "Loaded Chunks",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "count(container_last_seen{pod=~\"$pod\", container=\"papermc\"} \u003e (time() - 60))",
          "legendFormat": "Status",
          "refId": ""
        }
      ],
      "title": "Status",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "targets": [
        {
          "expr": "time() - container_start_time_seconds{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Uptime",
          "refId": ""
        }
      ],
      "title": "Uptime",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 20,
        "y": 0
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "mc_tps{pod=~\"$pod\"}",
          "legendFormat": "TPS",
          "refId": ""
        }
      ],
      "title": "TPS Over Time",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 20
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "mc_tick_duration_average{pod=~\"$pod\"} / 1000000",
          "legendFormat": "Average",
          "refId": ""
        },
        {
          "expr": "mc_tick_duration_median{pod=~\"$pod\"} / 1000000",
          "legendFormat": "Median",
          "refId": ""
        },
        {
          "expr": "mc_tick_duration_max{pod=~\"$pod\"} / 1000000",
          "legendFormat": "Max",
          "refId": ""
        }
      ],
      "title": "Tick Duration",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 5
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "mc_world_size{pod=~\"$pod\"}",
          "legendFormat": "{{world}}",
          "refId": ""
        }
      ],
      "title": "World Size",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 13
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "mc_players_online_total{pod=~\"$pod\"}",
          "legendFormat": "{{world}}",
          "refId": ""
        }
      ],
      "title": "Players per World",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 13
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "decimals": 0,
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "mc_jvm_memory{pod=~\"$pod\", type=\"allocated\"}",
          "legendFormat": "Allocated",
          "refId": ""
        },
        {
          "expr": "mc_jvm_memory{pod=~\"$pod\", type=\"max\"}",
          "legendFormat": "Max",
          "refId": ""
        },
        {
          "expr": "mc_jvm_memory{pod=~\"$pod\", type=\"free\"}",
          "legendFormat": "Free",
          "refId": ""
        }
      ],
      "title": "JVM Memory",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 21
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mc_jvm_gc_collection_seconds_sum{pod=~\"$pod\"}[5m])",
          "legendFormat": "{{gc}}",
          "refId": ""
        }
      ],
      "title": "JVM GC",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 21
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"$pod\", container=\"papermc\"}[5m]))",
          "legendFormat": "CPU Usage",
          "refId": ""
        },
        {
          "expr": "sum(container_spec_cpu_quota{pod=~\"$pod\", container=\"papermc\"} / container_spec_cpu_period{pod=~\"$pod\", container=\"papermc\"})",
          "legendFormat": "Limit",
          "refId": ""
        }
      ],
      "title": "Container CPU",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 29
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Working Set",
          "refId": ""
        },
        {
          "expr": "container_spec_memory_limit_bytes{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Limit",
          "refId": ""
        }
      ],
      "title": "Container Memory",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 29
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_network_receive_bytes_total{pod=~\"$pod\"}[5m]))",
          "legendFormat": "Receive",
          "refId": ""
        },
        {
          "expr": "sum(rate(container_network_transmit_bytes_total{pod=~\"$pod\"}[5m]))",
          "legendFormat": "Transmit",
          "refId": ""
        }
      ],
      "title": "Network Traffic",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 37
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_fs_reads_bytes_total{pod=~\"$pod\", container=\"papermc\"}[5m]))",
          "legendFormat": "Read",
          "refId": ""
        },
        {
          "expr": "sum(rate(container_fs_writes_bytes_total{pod=~\"$pod\", container=\"papermc\"}[5m]))",
          "legendFormat": "Write",
          "refId": ""
        }
      ],
      "title": "Disk I/O",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 37
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      }
    },
    {
      "type": "logs",
      "targets": [
        {
          "expr": "{kubernetes_namespace_name=\"paper\", kubernetes_pod_name=~\"$pod\"} | json | line_format \"{{.log}}\" | regexp `^\\[[\\d:]+\\s+(?P\u003clevel\u003e\\w+)\\]:\\s*(?:\\[(?P\u003cplugin\u003e[^\\]]+)\\]\\s*)?(?P\u003cmessage\u003e.*)` | level=~\"$level\" | plugin=~\"$plugin\" | line_format \"{{if .plugin}}[{{.plugin}}] {{end}}{{.message}}\"",
          "refId": ""
        }
      ],
      "title": "Server Logs",
      "transparent": false,
      "datasource": {
        "uid": "${loki}"
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 45
      },
      "transformations": [],
      "options": {
        "showLabels": false,
        "showCommonLabels": false,
        "showTime": true,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": ""
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": [
      {
        "id": "00000000-0000-0000-0000-000000000000",
        "type": "datasource",
        "name": "datasource",
        "label": "Prometheus",
        "hide": 0,
        "skipUrlSync": false,
        "query": "prometheus",
        "multi": false,
        "includeAll": false,
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      },
      {
        "id": "00000000-0000-0000-0000-000000000000",
        "type": "datasource",
        "name": "loki",
        "label": "Loki",
        "hide": 0,
        "skipUrlSync": false,
        "query": "loki",
        "multi": false,
        "includeAll": false,
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      },
      {
        "id": "00000000-0000-0000-0000-000000000000",
        "type": "query",
        "name": "pod",
        "label": "Pod",
        "hide": 0,
        "skipUrlSync": false,
        "query": "label_values(mc_tps, pod)",
        "datasource": {
          "uid": "${datasource}"
        },
        "multi": false,
        "refresh": 1,
        "includeAll": false,
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      },
      {
        "id": "00000000-0000-0000-0000-000000000000",
        "type": "custom",
        "name": "level",
        "label": "Log Level",
        "hide": 0,
        "skipUrlSync": false,
        "query": "INFO,WARN,ERROR",
        "multi": false,
        "includeAll": true,
        "allValue": ".*",
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      },
      {
        "id": "00000000-0000-0000-0000-000000000000",
        "type": "query",
        "name": "plugin",
        "label": "Plugin",
        "hide": 0,
        "skipUrlSync": false,
        "query": "{kubernetes_namespace_name=\"paper\"} | json | line_format \"{{.log}}\" | regexp `^\\[[\\d:]+\\s+\\w+\\]:\\s*\\[(?P\u003cplugin\u003e[^\\]]+)\\]`",
        "datasource": {
          "uid": "${loki}"
        },
        "multi": false,
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      }
    ]
  },
  "annotations": {}
}