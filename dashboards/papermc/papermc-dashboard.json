      "title": "CPU Throttling",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Working Set",
          "refId": ""
        },
        {
          "expr": "container_memory_rss{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "RSS",
          "refId": ""
        },
        {
          "expr": "container_memory_cache{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Cache",
          "refId": ""
        },
        {
          "expr": "container_spec_memory_limit_bytes{pod=~\"$pod\", container=\"papermc\"}",
          "legendFormat": "Limit",
          "refId": ""
        }
      ],
      "title": "Memory Usage",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "transformations": [],
      "options": null,
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{pod=~\"$pod\", container=\"papermc\"}) / sum(container_spec_memory_limit_bytes{pod=~\"$pod\", container=\"papermc\"}) * 100",
          "legendFormat": "Memory %",
          "refId": ""
        }
      ],
      "title": "Memory vs Limit",
      "transparent": false,
      "datasource": {
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
